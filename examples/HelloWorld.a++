
message = "Hello World!\n\0"

proc main
{
    rax = SYS_WRITE
    rdi = STDOUT
    rsi = message
    rdx = len(message)
    syscall

    rax = 0b110100 ; Testing Binary Numbers (52)
    rax = 60       ; Testing Normal Numbers (60)
    rax = 0x123    ; Testing Hex Numbers    (291)

    rax = SYS_EXIT
    rdi = 0x00
    syscall
}
